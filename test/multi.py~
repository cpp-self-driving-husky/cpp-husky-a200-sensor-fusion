import numpy as np


def multiplyABAt(a,b):
	A = np.array(a)
	B = np.array(b)
	AB = np.dot(A,B)
	ABAt = np.dot(AB,A.T)
	print(A)
	print(B)
	print(ABAt)
	

def multiply_test():
	
	mtxA = [[6,5,7,3],
			[7,3,9,8],
			[2,5,1,4]]
	mtxB = [[1,7,4,9],
			[6,5,1,6],
			[7,8,4,1],
			[1,2,7,4]]
	multiplyABAt(mtxA,mtxB)
	
	mtxC = [[6, 5, 7],
			[3, 7, 3],
			[9, 8, 2],
			[5, 1, 4],
			[7, 2, 3]]
	mtxD = [[1, 7, 4],
			[9, 6, 5],
			[1, 6, 7]]
	multiplyABAt(mtxC,mtxD)



